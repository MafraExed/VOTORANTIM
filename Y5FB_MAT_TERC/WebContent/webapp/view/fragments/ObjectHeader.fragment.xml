	<ObjectHeader id="idObjHead" 
				  title="{TitleHeader}"
				  iconDensityAware="true"
				  
				  numberState="{Infostate}" 
				  titleLevel="H1">
		<attributes> 
			<!-- <ObjectAttribute title="{i18n>objHeaderQtdUnit}"
							 text="{QtdItensHeader}"/>
							 
							 
			<ObjectAttribute title="{i18n>objHeaderVlr}"
							 text="{NumberHeader}"/>	
				 -->			 
							 
			<ObjectAttribute text="{DescriptionHeader1}"/>	
			<ObjectAttribute text="{DescriptionHeader2}"/>	
			<ObjectAttribute text="{DescriptionHeader3}"/>	
										
							 
		 </attributes>
		
		<additionalNumbers>
			<ObjectNumber number="{NumberHeader}" 
						  tooltip="{i18n>objHeaderQtd}" 
						  state="{Infostate}" 
