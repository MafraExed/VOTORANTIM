<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form">
	<Dialog title="{i18n>APPROVE}" type="Message" state="Success">
		<content>
			<f:SimpleForm>
				<f:content>
					<Text id="txtMApprove" text="{i18n>MASSAPPROVE}" />
				</f:content>
			</f:SimpleForm>
		</content>
		<beginButton>
			<Button text="{i18n>CONFIRM}" press="onConfirmApprove" />
		</beginButton>
		<endButton>
			<Button text="{i18n>CANCEL}" press="onCloseDialog" />
		</endButton>
	</Dialog>
</core:FragmentDefinition>