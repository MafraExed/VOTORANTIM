<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<TableSelectDialog id="TableSelectDialogID" noDataText="{i18n>NotFound}" title="{i18n>title_NFItem_Fragment}" search="handleSearchNFItem"
		confirm="handleCloseNFItemFragment" cancel="handleCloseNFItemFragment" items="{GE>/ZET_VCMM_NFITEMSet}">
		<columns>
			<Column>
				<header>
					<Text text="{i18n>title_Column_Itens_Item_NFe}"/>
				</header>
			</Column>
			<Column minScreenWidth="Desktop" demandPopin="true" popinDisplay="Inline">
				<header>
					<Text text="{i18n>title_Column_Itens_purchase_order}"/>
				</header>
			</Column>
			<Column minScreenWidth="Desktop" demandPopin="true" popinDisplay="Inline">
				<header>
					<Text text="{i18n>title_Column_Itens_purchase_order_item}"/>
				</header>
			</Column>
			<Column>
				<header>
					<Text text="{i18n>title_Column_Itens_Material}"/>
				</header>
			</Column>
			<Column minScreenWidth="Desktop" demandPopin="true" popinDisplay="Inline">
				<header>
					<Text text="{i18n>title_Column_Itens_material_description}"/>
				</header>
			</Column>
			<Column>
				<header>
					<Text text="{i18n>title_Column_Itens_quantity}"/>
				</header>
			</Column>
		</columns>
		<ColumnListItem>
			<cells>
				<ObjectIdentifier title="{GE>Itmnum}"/>
				<Text text="{GE>ebeln}"/>
				<Text text="{GE>ebelp}"/>
				<Text text="{GE>matnr}"/>
				<Text text="{GE>maktx}"/>
				<Text text="{GE>mengeMat}"/>
			</cells>
		</ColumnListItem>
	</TableSelectDialog>
</core:FragmentDefinition>