<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	controllerName="CADASTROAPROVADORES.CADASTROAPROVADOR.controller.addParam">
	<SelectDialog
		
		title="Aprovador"
		class="sapUiPopupWithPadding"
		items="{/ZET_CBMM_CF_PARAMETSet}"
		modelContextChange="_onModelContextChangeParam"
		
		search="_handleValueHelpParam"
		confirm="_handleValueHelpClose"
		cancel="_handleValueHelpClose">
		
		<StandardListItem
			icon="{ProductPicUrl}"
			iconDensityAware="false"
			iconInset="false"
			title="{Parametro}"
			 />
	</SelectDialog>
</core:FragmentDefinition>