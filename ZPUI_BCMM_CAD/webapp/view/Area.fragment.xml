<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	controllerName="CADASTROAPROVADORES.CADASTROAPROVADOR.controller.add">
	<SelectDialog
		
		title="Carteira"
		class="sapUiPopupWithPadding"
		items="{/ZET_CBMM_CF_APROVSet}"
		modelContextChange="_onModelContextChangeArea"
		
		search="_handleValueHelpArea"
		confirm="_handleValueHelpClose"
		cancel="_handleValueHelpClose">
		
		<StandardListItem
			icon="{ProductPicUrl}"
			iconDensityAware="false"
			iconInset="false"
			title="{Carteira}"
			/>
			
	</SelectDialog>
</core:FragmentDefinition>