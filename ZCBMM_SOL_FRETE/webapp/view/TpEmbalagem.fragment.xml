<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		
		title="Tipos de Embalagem"
		class="sapUiPopupWithPadding"
		items="{/ZET_CBMM_CF_PARAMETSet}"
		
		search="_handleValueHelpTpEmbalagem"
		confirm="_Confirme_TpEmbalagem"
		cancel="_handleValueHelpClose"
		modelContextChange="_onModelContextChangeTpEmbalagem">
		
		<StandardListItem
			icon="{ProductPicUrl}"
			iconDensityAware="false"
			iconInset="false"
			title="{Valor}"/>
			
	</SelectDialog>
</core:FragmentDefinition>