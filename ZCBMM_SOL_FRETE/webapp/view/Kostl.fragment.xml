<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		
		title="Centro de Custo"
		class="sapUiPopupWithPadding"
		items="{/ZET_CBMM_CF_KOSTLSet}"
		search="_handleValueHelpKostl"
		confirm="_Confirme_Kostl"
		cancel="_handleValueHelpClose"
		>
		
		<StandardListItem
			icon="{ProductPicUrl}"
			iconDensityAware="false"
			iconInset="false"
			title="{Kostl}"
			description="{Ktext}" />
			
	</SelectDialog>
</core:FragmentDefinition>