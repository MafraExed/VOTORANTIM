<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		
		title="Perigoso"
		class="sapUiPopupWithPadding"
		items="{/ZET_CBMM_CF_PARAMETSet}"
		
		search="_handleValueHelpClose"
		confirm="_Confirme_Perigoso"
		cancel="_handleValueHelpClose"
		modelContextChange="_onModelContextChangePerigoso">
		
		
		<StandardListItem
			icon="{ProductPicUrl}"
			iconDensityAware="false"
			iconInset="false"
			title="{Valor}"/>
			
	</SelectDialog>
</core:FragmentDefinition>