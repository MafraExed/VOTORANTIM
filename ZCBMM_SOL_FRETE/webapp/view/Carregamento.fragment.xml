<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		
		title="Tipos de Carregamento"
		class="sapUiPopupWithPadding"
		items="{/ZET_CBMM_CF_PARAMETSet}"
		
		search="_handleValueHelpClose"
		confirm="_Confirme_Carregamento"
		cancel="_handleValueHelpClose"
		modelContextChange="_onModelContextChangeTpCarreg">
		
		
		<StandardListItem
			icon="{ProductPicUrl}"
			iconDensityAware="false"
			iconInset="false"
			title="{Valor}"/>
			
	</SelectDialog>
</core:FragmentDefinition>