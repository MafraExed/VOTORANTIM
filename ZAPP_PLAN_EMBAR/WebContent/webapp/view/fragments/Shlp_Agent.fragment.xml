<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<TableSelectDialog
		noDataText="{i18n>notRetrieveText}"
		title="{i18n>shlpAgentTitleAgent}"
		search="onSearchHelperAgent"
		confirm="onConfirmShlpAgent"
		items="{/ZET_FBSD_SearchVendorSet}">
		<ColumnListItem>
			<cells>
				<Text text="{Lifnr}" />
				<Text text="{Name1}" />
			</cells>
		</ColumnListItem>
		<columns>
			<Column>
				<header>
					<Text text="{i18n>shlpAgentLabelAgent}" />
				</header>
			</Column>
			<Column
				minScreenWidth="Tablet"
				demandPopin="true" >
				<header>
					<Text text="{i18n>shlpAgentLabelNameAgent}" />
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>