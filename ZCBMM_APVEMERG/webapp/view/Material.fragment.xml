<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		
		title="Materiais"
		class="sapUiPopupWithPadding"
		items="{/ZET_CBMM_CF_PARAMETSet}"
		
		search="_handleValueHelpClose"
		confirm="_Confirme_Material"
		cancel="_handleValueHelpClose"
		modelContextChange="_onModelContextChangeMaterial">
		
		
		<StandardListItem
			icon="{ProductPicUrl}"
			iconDensityAware="false"
			iconInset="false"
			title="{Valor}"
			description="{Parametro}" />
			
	</SelectDialog>
</core:FragmentDefinition>