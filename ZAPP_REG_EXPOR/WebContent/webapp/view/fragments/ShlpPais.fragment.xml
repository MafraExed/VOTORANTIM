<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<TableSelectDialog noDataText="{i18n>notRetrieveText}"
					   title="{i18n>shlpAgentTitlePais}"
					   search="onSearchHelperPais"
					   confirm="onConfirmShlpPais"
					   items="{/ZET_FBSD_SearchPaisSet}">
	
		<columns>
			<Column>
				<header>
					<Text text="{i18n>shlpPaisLabelPais}" />
				</header>
			</Column>
			<Column
				minScreenWidth="Tablet"
				demandPopin="true" >
				<header>
					<Text text="{i18n>shlpPaisLabelNamePais}" />
				</header>
			</Column>
		</columns>
	
		<ColumnListItem>
			<cells>
				<Text text="{Pais}" />
				<Text text="{Name1}" />
			</cells>
		</ColumnListItem>
	
					   	
	</TableSelectDialog>
</core:FragmentDefinition>