<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<TableSelectDialog
		noDataText="{i18n>notRetrieveText}"
		title="{i18n>shlpLocExpedicaoTitleUser}"
		search="onSearchHelperGlobal"
		confirm="onConfirmShlpVstel"
		items="{/ZET_FBSD_SearchLocalExpSet}">
		<ColumnListItem>
			<cells>
				<Text text="{Vstel}" />
				<Text text="{Name1}" />
			</cells>
		</ColumnListItem>
		<columns>
			<Column>
				<header>
					<Text text="{i18n>shlpLocExpedicaoLabelUser}" />
				</header>
			</Column>
			<Column
				minScreenWidth="Tablet"
				demandPopin="true" >
				<header>
					<Text text="{i18n>shlpLocExpedicaoLabelNameUser}" />
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>