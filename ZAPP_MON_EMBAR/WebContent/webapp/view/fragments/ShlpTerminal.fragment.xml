<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<TableSelectDialog
		noDataText="{i18n>notRetrieveText}"
		title="{i18n>shlpTerminalTitleUser}"
		search="onSearchHelperGlobal"
		confirm="onConfirmShlpDterm"
		items="{/ZET_FBSD_SearchTerminalSet}">
		<ColumnListItem>
			<cells>
				<Text text="{Dterm}" />
				<Text text="{DescTerm}" />
			</cells>
		</ColumnListItem>
		<columns>
			<Column>
				<header>
					<Text text="{i18n>shlpTerminalLabelUser}" />
				</header>
			</Column>
			<Column
				minScreenWidth="Tablet"
				demandPopin="true" >
				<header>
					<Text text="{i18n>shlpTerminalLabelNameUser}" />
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>