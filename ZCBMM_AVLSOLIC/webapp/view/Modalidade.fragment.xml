<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		
		title="Modalidades"
		class="sapUiPopupWithPadding"
		items="{/ZET_CBMM_CF_PARAMETSet}"
		
		search="_handleValueHelpModalidade"
		confirm="_handleValueHelpClose"
		cancel="_handleValueHelpClose"
		modelContextChange="_onModelContextChangeModalidade">
		
		<StandardListItem
			icon="{ProductPicUrl}"
			iconDensityAware="false"
			iconInset="false"
			title="{Valor}"
			description="{Parametro}" />
			
	</SelectDialog>
</core:FragmentDefinition>